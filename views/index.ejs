<%
const link="/data/"+((dirName)?(dirName)+'/':'')
%>
<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src='/javascripts/viewImageWhenShowUpByScrollDown.js' defer></script>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% for(let folder of folderNames){ %>
      <p><a href="<%=(dirName || '')%>/<%= encodeURI(folder) %>"><%= folder %></a></p>
    <% } %>    
    <% for(let img of imgNames){ %>
      <br/><img class="imgs" img-src="<%=(link+encodeURI(img))%>">
    <% } %>
  </body>
</html>
